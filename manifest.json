{
  "name": "CleanWiki",
  "description": "A clean white theme for Wikipedia",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*"], 
      "css" : ["wikistyle.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}